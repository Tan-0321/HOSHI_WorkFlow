# function
filt(i_in,i_set,x)= (i_in==i_set) ? x : 1/0
lg(x)             = (x>0.) ? log10(x) : ( (x<0.) ? log10(-x) : 0.)
log_pos(x)        = (x>0.) ? log10(x) : 1/0
log_neg(x)        = (x<0.) ? log10(-x): 1/0

Prot(rad,vrot)    = (6.28*rad*6.96e10)/(vrot*1e5)/(24*3600)
Beq(vel,dens)     =  vel * sqrt( 12.56*dens )
vrot(rad,omg)     =  rad*omg / 8.64e4

