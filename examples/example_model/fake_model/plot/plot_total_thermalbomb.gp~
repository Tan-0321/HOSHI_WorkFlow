
set xl 'time [sec]'
set yl 'Eexp [1e51]'
set xr []
set yr [*:*]

set y2l 'lg Jtot'
set ytics nomirror
set y2tics
set y2r [*:*]

plot \
file_summary           u (log10($5)):($8/1e51)     w l         ls  1 ti 'Etot',\
file_summary eve dnstg u (log10($5)):($8/1e51)     w p         ls  1 ti '',\

unset xr
unset yr

unset y2l
unset y2tics